const hre = require("hardhat");

async function main() {
  const ResearchNFT = await hre.ethers.getContractFactory("ResearchNFT");
  const researchNFT = await ResearchNFT.deploy();
  await researchNFT.deployed();

  console.log("ResearchNFT deployed to:", researchNFT.address);
}

main()
  .then(() => process.exit(0))
  .catch((error) => {
    console.error(error);
    process.exit(1);
  });
